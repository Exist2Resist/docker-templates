<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name>MariaDB</Name>
  <Beta>False</Beta>
  <Category>Tools:Database</Category>
  <Overview>
  library/mariadb:tag[br][br]
	Docker image for MariaDB[br][br]	
	
	
	[span style=\'color: #E80000;\'][/span][br][br]	
	
  </Overview>
  <Registry>https://hub.docker.com/_/mariadb/</Registry>
  <GitHub>https://github.com/docker-library/mariadb</GitHub>
  <Repository>library/mariadb</Repository>
  <Support>n/a</Support>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Network>host</Network>
  <WebUI></WebUI>
  <Icon>http://i.imgur.com/nOIZcJ1.png?1</Icon>
  <ExtraParams></ExtraParams>
  <Config Type="Path" Name="Config directory" Target="/var/lib/mysql" Mode="rw" Description="Optional: Host path to store MariaDB files. Optional" Display="always" Required="false"></Config>
  <Config Type="Port" Name="TCP Port" Target="3306" Default="3306" Mode="tcp" Description="TCP port 1011" Display="always" Required="false">3306</Config>
  <Config Type="Variable" Name="SQL root password" Target="MYSQL_ROOT_PASSWORD" Default="r00t!" Description="Root password" Display="always" Required="true"></Config>
  <Config Type="Variable" Name="SQL User name" Target="MYSQL_USER" Description="Optional: User name" Display="always" Required="false"></Config>
  <Config Type="Variable" Name="SQL User password" Target="MYSQL_PASSWORD" Description="Optional: User password" Display="always" Required="false"></Config>
  
  <Config Type="Variable" Name="Run with user ID" Target="APP_UID" Default="99" Description="Run application with specified user id." Mask="false" Display="advanced" Required="true">99</Config>
  <Config Type="Variable" Name="Run with group ID" Target="APP_GID" Default="100" Description="Run application with specified group id." Mask="false" Display="advanced" Required="true">99</Config>
</Container>
